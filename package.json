    "url": "git+https://github.com/josephmasongsong/super-simple-ui-components.git"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "node-sass": "^8.0.0",
    "rollup": "^3.21.5",
    "rollup-plugin-scss": "^4.0.0",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4"
  }
